{
    "name": "uncleversace/maintenance-bundle",
    "description": "Provides maintenance mode functionality for Symfony applications",
    "type": "symfony-bundle",
    "keywords": [
        "symfony",
        "bundle",
        "maintenance",
        "downtime"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Uncle Versace",
            "email": "uncleversace@proton.me",
            "homepage": "https://uncleversace.xyz/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "8.1.* || 8.2.* || 8.3.* || 8.4.*",
        "symfony/console": "^6.3|^7.0",
        "symfony/filesystem": "^6.3|^7.0",
        "symfony/framework-bundle": "^6.3|^7.0",
        "twig/twig": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "UncleVersace\\MaintenanceBundle\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.3"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.3|^7.0"
        }
    }
}
